<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.grays2.system.mapper.UserRoleMapper">
    <insert id="insertUserRole" parameterType="com.grays2.system.domain.UserRole">
        insert into sys_user_role (user_id, role_id)
        values (#{userId}, #{roleId})
    </insert>
    <update id="updateUserRole" parameterType="com.grays2.system.domain.UserRole">
    </update>
    <select id="getLastId" resultType="java.lang.Integer">
        select id
        from sys_user_role
        order by id desc
        limit 1
    </select>
</mapper>
