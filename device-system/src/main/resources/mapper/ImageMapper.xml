<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.grays2.system.mapper.ImageMapper">

    <select id="getLast" resultType="com.grays2.system.domain.Image">
        select *
        from sys_image
        order by id desc
        limit 1
    </select>
</mapper>
